extends ./layouts/layout.jade

block content
    include pages/index/first-screen.jade
    section.section.section_programs
        .section_programs-bg-first
        .container
            .row
                .col-12
                    h2.title.title_second Эфирные программы
            .row.js-slider-programs
                - for(var x = 0; x < 8; x++)
                    .col-3.program__wrap
                        a(href='#' style='background: linear-gradient(180deg, rgba(115, 60, 245, 0) 50.18%, #733CF5 100%)').program
                            .program__image
                                img(src='../img/program.png')
                            .program__content Музыкальный эфир
    section.section.section_new
        .container
            .row
                .col-12
                    h2.title.title_second Что нового?
            .news__slider.js-slider-new.row
                - for(var x = 0; x < 8; x++)
                    a(href='#').new__item.new__item_index
                        .new__image.new__image_index
                            img(src='../img/news-item.jpg')
                        .new__content.new__content_index Открытие нового объекта в Советском районе
    .section.section_instagram
        .container
            .row
                .col-12.col-lg-6
                    a(href='#').instagram__item(style='background-color: #F8AB32; background-image: url(./img/inst1.png)')
                        .instagram__content.instagram__content_white Творческий вечер в Labirint
                .col-12.col-lg-6
                    a(href='#').instagram__item(style='background-color: #7CFA99; background-image: url(./img/inst2.png)')
                        .instagram__content.instagram__content_dark Enegry эфир с основателем Q
    section.section.section_news
        .container
            .row
                .col-12
                    h2.title.title_second Новости
            .row
                .col-12.col-md-8
                    .news__wrapper
                        .row.js-slider-news
                            - for(var x = 0; x < 5;x++)
                                a(href='#').news__item.col-3.col-md-12
                                    .row
                                        .col-12.col-md-2
                                            .news__image
                                                img(src='../img/news.png')
                                        .col-12.col-md-7
